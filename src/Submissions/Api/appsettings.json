{
  "ConnectionStrings": {
    "Default": "Server=(localdb)\\MSSQLLocalDB;Database=Biobanks;Trusted_Connection=True;MultipleActiveResultSets=true",
    "AzureStorage": "UseDevelopmentStorage=true;"
  },

  "Swagger": {
    "Filename": "swagger.xml"
  },

  "JWT": {
    "Issuer": "UKCRC-DirectoryIdP",
    "Audience": "UKCRC-DirectoryApi"
  },

  "Limits": {
    "EntitiesPerSubmission": 10000
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "Analytics": {
    "ApiKey": "",
    "ViewId": "",
    "DirectoryHostname": "",
    "StartDate": "2016-01-01",
    "MetricThreshold": 10,
    "EventThreshold": 30,
    "FilterByHost": true
  },

  "Publications": {
    "EpmcApiUrl": "https://www.ebi.ac.uk/europepmc/"
  },

    "AllowedHosts": "*"
  }
