{
  // Where we're posting data to, and our auth token
  "DataServiceUri": "",
  "BearerToken": "",

  // path to the SNOMED.csv as it's not stored with the application
  // like the other reference data
  "SnomedFile": "snomed.csv",

  // Maps tag values found in the SNOMED csv (the key)
  // to friendly values stored at the data service (the value)
  "TagMappings": {
    "BodyOrgan": "Body Organ",
    "CollectionProcedure": "Extraction Procedure",
    "BloodMaterial": "Blood Product"
  },

  // TODO Change this to something more sensible e.g. build configs
  "env":  "Development" 
}
